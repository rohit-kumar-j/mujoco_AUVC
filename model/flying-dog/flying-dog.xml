<mujoco model="Flying-Dog">
  <compiler autolimits="true" angle="degree"/>
  <!-- <option gravity="0 0 0" /> -->
  <option gravity="0 0 -9.81" />

  <asset>
    <mesh name="chassis" scale="0.005 0.005 0.005" file="frame.obj" />
    <mesh name="motor_AX18" scale="0.05 0.05 0.05" file="AX12.stl" />
    <mesh name="prop" scale="0.005 0.005 0.005" file="propeller_94.STL" />
    <mesh name="bldc1" scale="0.005 0.005 0.005" file="propeller_MT2208.STL" />
    <mesh name="bldc2" scale="0.005 0.005 0.005" file="propeller_ADAPTER.STL" />
    <mesh name="spinner" scale="0.005 0.005 0.005" file="propeller_spinner.STL" />

    <mesh name="coxa2" scale="0.005 0.005 0.005" file="coxa2.STL" />
    <mesh name="femur2" scale="0.005 0.005 0.005" file="femur2.STL" />
    <mesh name="tibia2" scale="0.005 0.005 0.005" file="tibia2.STL" />

    <mesh name="coxa_AX18" scale="0.005 0.005 0.005" file="coxa_AX18.STL" />
    <mesh name="coxa_MX28" scale="0.005 0.005 0.005" file="coxa_MX28.STL" />
    <mesh name="femur_AX18" scale="0.005 0.005 0.005" file="femur_AX18.STL" />
    <mesh name="femur_MX28" scale="0.005 0.005 0.005" file="femur_MX28.STL" />
    <mesh name="tibia_AX18" scale="0.005 0.005 0.005" file="tibia_AX18.STL" />
    <mesh name="tibia_MX28" scale="0.005 0.005 0.005" file="tibia_MX28.STL" />
    <mesh name="body_hexa_AX18" scale="0.005 0.005 0.005" file="body_hexa_AX18.STL" />
    <mesh name="body_hexa_MX28" scale="0.005 0.005 0.005" file="body_hexa_MX28.STL" />
    <mesh name="body_quad_AX18" scale="0.005 0.005 0.005" file="body_quad_AX18.STL" />
    <mesh name="body_quad_MX28" scale="0.005 0.005 0.005" file="body_quad_MX28.STL" />
    <mesh name="rubber_foot" scale="0.005 0.005 0.005" file="Rubber_Foot.STL" />

    <texture name="grid" type="2d" builtin="checker" width="512" height="512" rgb1=".1 .2 .3" rgb2=".2 .3 .4"/>
    <material name="grid" texture="grid" texrepeat="1 1" texuniform="true" reflectance=".2"/>
  </asset>

  <default>
    <!-- <motor ctrlrange="-33.5 33.5" ctrllimited="true"/> -->
    <joint type="hinge" stiffness="5" damping="10" frictionloss="3"/>
    <site type="cylinder" size="0.01 0.01"/>
  </default>

  <worldbody>
    <geom name="floor" size="0 0 .05" type="plane" material="grid" condim="3"/>
    <light name="spotlight" mode="targetbodycom" castshadow="false" target="fdog" diffuse=".8 .8 .8" specular="0.3 0.3 0.3" pos="0 -6 4" cutoff="30"/>
    <body name="fdog" pos="0 0 .8">
      <freejoint/>
      <light name="top light1" castshadow="false" pos="1 1 1" mode="targetbodycom" target="fdog" diffuse=".4 .4 .4"/>
      <light name="top light2" castshadow="false" pos="-1 1 1" mode="targetbodycom" target="fdog" diffuse=".4 .4 .4"/>
      <light name="top light3" castshadow="false" pos="-1 -1 1" mode="targetbodycom" target="fdog" diffuse=".4 .4 .4"/>
      <light name="top light4" castshadow="false" pos="1 -1 1" mode="targetbodycom" target="fdog" diffuse=".4 .4 .4"/>

      <geom name="chasis1" type="mesh" mesh="chassis"  pos="0 0 0.176" />
      <geom name="chasis2" type="mesh" mesh="chassis" />
      <body name="front_camera_100"  euler="85 0 0" pos="0 0.4 0.3">
        <camera name="front_cam" mode="fixed" fovy="90"/>
        <geom type="box" size="0.15 0.12 0.07" mass="0.0001" rgba="1 0 0 1"/>
        <geom type="cylinder" size="0.06 0.07" mass="0.0001" pos="0 0 -0.1" rgba="0 1 0 1"/>
      </body>
      <!-- <body name="front_camera_45"  euler="-85 0 180" pos="0 -0.4 0.1"> -->
      <!--   <camera name="front_cam" mode="fixed" fovy="45"/> -->
      <!--   <geom type="box" size="0.15 0.12 0.07" mass="0.0001" rgba="1 0 0 1"/> -->
      <!--   <geom type="cylinder" size="0.06 0.07" mass="0.0001" pos="0 0 -0.1" rgba="0 1 0 1"/> -->
      <!-- </body> -->
      <geom name="fl_hip_motor" type="mesh" mesh="motor_AX18" pos="0.325 0.602 0.096" euler="0 0 -135" rgba="0 0 0 1"/>
      <geom name="fr_hip_motor" type="mesh" mesh="motor_AX18" pos="0.325 -0.602 0.096" euler="0 0 135" rgba="0 0 0 1"/>
      <geom name="br_hip_motor" type="mesh" mesh="motor_AX18" pos="-0.325 0.602 0.096" euler="0 0 -45" rgba="0 0 0 1"/>
      <geom name="bl_hip_motor" type="mesh" mesh="motor_AX18" pos="-0.325 -0.602 0.096" euler="0 0 45" rgba="0 0 0 1"/>
      <body name="fr_thigh" pos="0.326 0.598 0.095" euler="0 0 45">
        <joint name="fr_1_hinge" type="hinge" range="-45 45" limited="true"/>
        <geom name="fr_thigh_geom" type="mesh" mesh="coxa2" pos="-0.066 -0.115 -0.12" rgba="1 0.54 0 1"/>
        <body name="fr_mid_link" pos="0.46 0 0" euler="90 0 0" >
          <joint name="fr_2_hinge" type="hinge" range="-120 45" limited="true" />
          <geom name="fr_thigh_motor" type="mesh" mesh="motor_AX18" pos="0 0 0" euler="0 0 0" rgba="0 0 0 1"/>
          <geom name="fr_thigh_link" type="mesh" mesh="femur2" pos="-0.05 0.108 -0.12" euler="0 0 -90" rgba="1 0.54 0 1"/>
          <geom name="fr_BLDC" type="mesh" mesh="bldc1" pos="-0.545 0.1 -0.618" rgba="0.4 0.5 0.8 1" />
          <body name="fr_prop" pos="0.074 0.24 0" euler="-90 0 0">
            <site name="fr_s1"/>
            <joint name="fr_prop_hinge" type="hinge" />
            <geom name="fr_prop" type="mesh" mesh="prop" pos="-0.62 0.619 -.18" euler="90 0 0" rgba="0.2 0.4 0.8 1" />
            <geom name="fr_prop_spinner" type="mesh" mesh="spinner" pos="-0.62 0.619 -.18" euler="90 0 0" rgba="0.5 0 0 1" />
          </body>
          <body name="fr_final_link" pos="0.326 -0.075 0." euler="0 0 -45">
            <joint name="fr_3_hinge" type="hinge" axis="0 0 -1" range="-45 70" limited="true"/>
            <!-- <geom name="ref_cylinder" type="cylinder" size="0.01 0.2"/> -->
            <!-- <geom name="ref_sphere" type="sphere" size="0.03"/> -->
            <geom name="fr_final_motor" type="mesh" mesh="motor_AX18" rgba="0 0 0 1"/>
            <geom name="fr_final_link" type="mesh" mesh="tibia2" pos="0.433 -0.6 0.125" euler="180 0 180" rgba="1 0.54 0 1"/>
          </body>
        </body>
      </body>
      <body name="fl_thigh" pos="-0.326 0.598 0.095" euler="0 0 135">
        <!-- TODO: This leg's joints-->
        <joint name="fl_1_hinge" type="hinge" range="-45 45" limited="true" />
        <geom name="fl_thigh_geom" type="mesh" mesh="coxa2" pos="-0.066 -0.115 -0.12" rgba="1 0.54 0 1"/>
        <body name="fl_mid_link" pos="0.46 0 0" euler="90 0 0" >
          <joint name="fl_2_hinge" type="hinge" axis="0 0 -1" range="-45 120" limited="true" />
          <geom name="fl_thigh_motor" type="mesh" mesh="motor_AX18" pos="0 0 0" euler="0 0 0" rgba="0 0 0 1"/>
          <geom name="fl_thigh_link" type="mesh" mesh="femur2" pos="-0.05 0.108 -0.12" euler="0 0 -90" rgba="1 0.54 0 1"/>
          <geom name="fl_BLDC" type="mesh" mesh="bldc1" pos="-0.545 0.1 -0.618" rgba="0.4 0.5 0.8 1" />
          <body name="fl_prop" pos="0.074 0.24 0" euler="-90 0 0">
            <site name="fl_s1" />
            <joint name="fl_prop_hinge" type="hinge" />
            <geom name="fl_prop" type="mesh" mesh="prop" pos="-0.62 0.619 -.18" euler="90 0 0" rgba="0.2 0.4 0.8 1" />
            <geom name="fl_prop_spinner" type="mesh" mesh="spinner" pos="-0.62 0.619 -.18" euler="90 0 0" rgba="0.5 0 0 1" />
          </body>
          <body name="fl_final_link" pos="0.326 -0.075 0." euler="0 0 -45">
            <joint name="fl_3_hinge" type="hinge" axis="0 0 -1" range="-45 70" limited="true" />
            <!-- <geom name="ref_cylinder" type="cylinder" size="0.01 0.2"/> -->
            <!-- <geom name="ref_sphere" type="sphere" size="0.03"/> -->
            <geom name="fl_final_motor" type="mesh" mesh="motor_AX18" rgba="0 0 0 1"/>
            <geom name="fl_final_link" type="mesh" mesh="tibia2" pos="0.433 -0.6 0.125" euler="180 0 180" rgba="1 0.54 0 1"/>
          </body>
        </body>
      </body>
      <body name="br_thigh" pos="0.326 -0.598 0.095" euler="0 0 -45">
        <joint name="br_1_hinge" type="hinge" range="-45 45" limited="true" />
        <geom name="br_thigh_geom" type="mesh" mesh="coxa2" pos="-0.066 -0.115 -0.12" rgba="1 0.54 0 1"/>
        <body name="br_mid_link" pos="0.46 0 0" euler="90 0 0" >
          <joint name="br_2_hinge" type="hinge" axis="0 0 -1" range="-45 120" limited="true" />
          <geom name="br_thigh_motor" type="mesh" mesh="motor_AX18" pos="0 0 0" euler="0 0 0" rgba="0 0 0 1"/>
          <geom name="br_thigh_link" type="mesh" mesh="femur2" pos="-0.05 0.108 -0.12" euler="0 0 -90" rgba="1 0.54 0 1"/>
          <geom name="br_BLDC" type="mesh" mesh="bldc1" pos="-0.545 0.1 -0.618" rgba="0.4 0.5 0.8 1" />
          <body name="br_prop" pos="0.074 0.24 0" euler="-90 0 0">
            <site name="br_s1" />
            <joint name="br_prop_hinge" type="hinge" />
            <geom name="br_prop" type="mesh" mesh="prop" pos="-0.62 0.619 -.18" euler="90 0 0" rgba="0.2 0.4 0.8 1" />
            <geom name="br_prop_spinner" type="mesh" mesh="spinner" pos="-0.62 0.619 -.18" euler="90 0 0" rgba="0.5 0 0 1" />
          </body>
          <body name="br_final_link" pos="0.326 -0.075 0." euler="0 0 -45">
            <joint name="br_3_hinge" type="hinge" axis="0 0 -1" range="-45 70" limited="true"/>
            <!-- <geom name="ref_cylinder" type="cylinder" size="0.01 0.2"/> -->
            <!-- <geom name="ref_sphere" type="sphere" size="0.03"/> -->
            <geom name="br_final_motor" type="mesh" mesh="motor_AX18" rgba="0 0 0 1"/>
            <geom name="br_final_link" type="mesh" mesh="tibia2" pos="0.433 -0.6 0.125" euler="180 0 180" rgba="1 0.54 0 1"/>
          </body>
        </body>
      </body>
      <body name="bl_thigh" pos="-0.326 -0.598 0.095" euler="0 0 225">
        <joint name="bl_1_hinge" type="hinge" range="-45 45" limited="true" />
        <geom name="bl_thigh_geom" type="mesh" mesh="coxa2" pos="-0.066 -0.115 -0.12" rgba="1 0.54 0 1"/>
        <body name="bl_mid_link" pos="0.46 0 0" euler="90 0 0" >
          <joint name="bl_2_hinge" type="hinge"  axis="0 0 -1" range="-45 120" limited="true"/>
          <geom name="bl_thigh_motor" type="mesh" mesh="motor_AX18" pos="0 0 0" euler="0 0 0" rgba="0 0 0 1"/>
          <geom name="bl_thigh_link" type="mesh" mesh="femur2" pos="-0.05 0.108 -0.12" euler="0 0 -90" rgba="1 0.54 0 1"/>
          <geom name="bl_BLDC" type="mesh" mesh="bldc1" pos="-0.545 0.1 -0.618" rgba="0.4 0.5 0.8 1" />
          <body name="bl_prop" pos="0.074 0.24 0" euler="-90 0 0">
            <site name="bl_s1" />
            <joint name="bl_prop_hinge" type="hinge" />
            <geom name="bl_prop" type="mesh" mesh="prop" pos="-0.62 0.619 -.18" euler="90 0 0" rgba="0.2 0.4 0.8 1" />
            <geom name="bl_prop_spinner" type="mesh" mesh="spinner" pos="-0.62 0.619 -.18" euler="90 0 0" rgba="0.5 0 0 1" />
          </body>
          <body name="bl_final_link" pos="0.326 -0.075 0." euler="0 0 -45">
            <joint name="bl_3_hinge" type="hinge" axis="0 0 -1" range="-45 70" limited="true"/>
            <!-- <geom name="ref_cylinder" type="cylinder" size="0.01 0.2"/> -->
            <!-- <geom name="ref_sphere" type="sphere" size="0.03"/> -->
            <geom name="bl_final_motor" type="mesh" mesh="motor_AX18" rgba="0 0 0 1"/>
            <geom name="bl_final_link" type="mesh" mesh="tibia2" pos="0.433 -0.6 0.125" euler="180 0 180" rgba="1 0.54 0 1"/>
          </body>
        </body>
      </body>
    </body>
  </worldbody>

  <actuator>
    <position name="fr_1_act" joint="fr_1_hinge" kp="288" kv="100" gear="10" forcelimited="true" forcerange="-150 150"/>
    <position name="fr_2_act" joint="fr_2_hinge" kp="288" kv="100" gear="10" forcelimited="true" forcerange="-180 180"/>
    <position name="fr_3_act" joint="fr_3_hinge" kp="288" kv="100" gear="10" forcelimited="true" forcerange="-150 150"/>
    <velocity name="fr_prop" joint="fr_prop_hinge" kv="40" gear="5" ctrllimited="false" ctrlrange="-200 0"/>
    <motor name="fr_prop_motor"  site="fr_s1" gear="0 0 1 0 0 0" ctrllimited="false" ctrlrange="-952 952" forcelimited="false" forcerange="-951 951"/>

    <position name="fl_1_act" joint="fl_1_hinge" kp="288" kv="100" gear="10" forcelimited="true" forcerange="-150 150"/>
    <position name="fl_2_act" joint="fl_2_hinge" kp="288" kv="100" gear="10" forcelimited="true" forcerange="-180 180"/>
    <position name="fl_3_act" joint="fl_3_hinge" kp="288" kv="100" gear="10" forcelimited="true" forcerange="-150 150"/>
    <velocity name="fl_prop" joint="fl_prop_hinge" kv="40" gear="5" ctrllimited="false" ctrlrange="0 200"/>
    <motor name="fl_prop_motor"  site="fl_s1" gear="0 0 1 0 0 0" ctrllimited="false" ctrlrange="-952 952" forcelimited="false" forcerange="-951 951"/>

    <position name="br_1_act" joint="br_1_hinge" kp="288" kv="100" gear="10" forcelimited="true" forcerange="-150 150"/>
    <position name="br_2_act" joint="br_2_hinge" kp="288" kv="100" gear="10" forcelimited="true" forcerange="-180 180"/>
    <position name="br_3_act" joint="br_3_hinge" kp="288" kv="100" gear="10" forcelimited="true" forcerange="-150 150"/>
    <velocity name="br_prop" joint="br_prop_hinge" kv="40" gear="5" ctrllimited="false" ctrlrange="0 200"/>
    <motor name="br_prop_motor"  site="br_s1" gear="0 0 1 0 0 0" ctrllimited="false" ctrlrange="-952 952" forcelimited="false" forcerange="-951 951"/>

    <position name="bl_1_act" joint="bl_1_hinge" kp="288" kv="100" gear="10" forcelimited="true" forcerange="-150 150"/>
    <position name="bl_2_act" joint="bl_2_hinge" kp="288" kv="100" gear="10" forcelimited="true" forcerange="-180 180"/>
    <position name="bl_3_act" joint="bl_3_hinge" kp="288" kv="100" gear="10" forcelimited="true" forcerange="-150 150"/>
    <velocity name="bl_prop" joint="bl_prop_hinge" kv="40" gear="5" ctrllimited="false" ctrlrange="-200 0"/>
    <motor name="bl_prop_motor"  site="bl_s1" gear="0 0 1 0 0 0" ctrllimited="false" ctrlrange="-952 952" forcelimited="false" forcerange="-951 951"/>

  </actuator>

  <sensor>
    <jointpos name="fr_hip" joint="fr_1_hinge" />
    <jointpos name="fr_knee" joint="fr_2_hinge" />
    <jointpos name="fr_foot" joint="fr_3_hinge" />

    <jointpos name="fl_hip" joint="fl_1_hinge" />
    <jointpos name="fl_knee" joint="fl_2_hinge" />
    <jointpos name="fl_foot" joint="fl_3_hinge" />

    <jointpos name="br_hip" joint="br_1_hinge" />
    <jointpos name="br_knee" joint="br_2_hinge" />
    <jointpos name="br_foot" joint="br_3_hinge" />

    <jointpos name="bl_hip" joint="bl_1_hinge" />
    <jointpos name="bl_knee" joint="bl_2_hinge" />
    <jointpos name="bl_foot" joint="bl_3_hinge" />
  </sensor>

  <contact>
    <exclude body1="fdog" body2="fr_mid_link"/>
    <exclude body1="fdog" body2="fr_prop" />
    <exclude body1="fdog" body2="fr_thigh" />
    <exclude body1="fr_mid_link" body2="fr_prop"/>
    <exclude body1="fr_mid_link" body2="fr_final_link"/>
    <exclude body1="fr_mid_link" body2="fr_thigh"/>
    <exclude body1="fr_prop" body2="fr_final_link" />
    <exclude body1="fr_prop" body2="fr_thigh" />
    <exclude body1="fr_final_link" body2="fr_thigh" />

    <exclude body1="fdog" body2="fl_mid_link"/>
    <exclude body1="fdog" body2="fl_prop" />
    <exclude body1="fdog" body2="fl_thigh" />
    <exclude body1="fl_mid_link" body2="fl_prop"/>
    <exclude body1="fl_mid_link" body2="fl_final_link"/>
    <exclude body1="fl_mid_link" body2="fl_thigh"/>
    <exclude body1="fl_prop" body2="fl_final_link" />
    <exclude body1="fl_prop" body2="fl_thigh" />
    <exclude body1="fl_final_link" body2="fl_thigh" />

    <exclude body1="fdog" body2="bl_mid_link"/>
    <exclude body1="fdog" body2="bl_prop" />
    <exclude body1="fdog" body2="bl_thigh" />
    <exclude body1="bl_mid_link" body2="bl_prop"/>
    <exclude body1="bl_mid_link" body2="bl_final_link"/>
    <exclude body1="bl_mid_link" body2="bl_thigh"/>
    <exclude body1="bl_prop" body2="bl_final_link" />
    <exclude body1="bl_prop" body2="bl_thigh" />
    <exclude body1="bl_final_link" body2="bl_thigh" />

    <exclude body1="fdog" body2="br_mid_link"/>
    <exclude body1="fdog" body2="br_prop" />
    <exclude body1="fdog" body2="br_thigh" />
    <exclude body1="br_mid_link" body2="br_prop"/>
    <exclude body1="br_mid_link" body2="br_final_link"/>
    <exclude body1="br_mid_link" body2="br_thigh"/>
    <exclude body1="br_prop" body2="br_final_link" />
    <exclude body1="br_prop" body2="br_thigh" />
    <exclude body1="br_final_link" body2="br_thigh" />

    <exclude body1="fl_prop" body2="fr_prop"/>
    <exclude body1="bl_prop" body2="fr_prop"/>
    <exclude body1="br_prop" body2="fr_prop"/>
    <exclude body1="bl_prop" body2="fl_prop"/>
    <exclude body1="br_prop" body2="fl_prop"/>
    <exclude body1="br_prop" body2="bl_prop"/>

    <pair geom1="fr_final_link" geom2="floor" friction="1.5 2.5 1 1"/>
    <pair geom1="fl_final_link" geom2="floor" friction="1.5 2.5 1 1"/>
    <pair geom1="bl_final_link" geom2="floor" friction="1.5 1.5 1 1"/>
    <pair geom1="br_final_link" geom2="floor" friction="1.5 1.5 1 1"/>

    <!-- NOTE: Uncomment these lines to enable drv config 2 and disable it for drv config 1 -->
    <!-- <exclude body1="fdog" body2="fr_final_link" /> -->
    <!-- <exclude body1="fdog" body2="fl_final_link" /> -->
    <!-- <exclude body1="fdog" body2="bl_final_link" /> -->
    <!-- <exclude body1="fdog" body2="br_final_link" /> -->

  </contact>

</mujoco>
